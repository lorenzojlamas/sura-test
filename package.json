{
    "name": "test-sura",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "setup": "npm install",
        "clean": "shx rm -rf lib/",
        "clean:full": "shx rm -rf node_modules/ lib/ ",
        "build": "npm run clean && tsc",
        "lint": "esLint 'src/**/*.ts'",
        "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
        "start": "node ./lib/index.js",
        "test": "nyc npm run test-only",
        "test-only": "mocha --reporter cypress-multi-reporters --reporter-options configFile=mocha-reporters.json --require ts-node/register --require source-map-support/register 'src/**/*.spec.ts'",
        "watch-ts": "tsc -w",
        "watch-test": "mocha --require ts-node/register --require source-map-support/register --watch 'src/**/*.spec.ts' --watch-extensions ts --colors false",
        "watch-node": "nodemon --debug",
        "update-dependencies": "updtr -u npm -r basic"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/chai": "^4.2.14",
        "@types/express": "^4.17.9",
        "@types/mocha": "^8.2.0",
        "@types/node": "^14.14.20",
        "@types/sinon": "^9.0.10",
        "@typescript-eslint/eslint-plugin": "^4.12.0",
        "@typescript-eslint/parser": "^4.12.0",
        "chai": "^4.2.0",
        "cypress-multi-reporters": "^1.4.0",
        "docdash": "^1.2.0",
        "eslint": "^7.17.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsdoc": "^30.7.13",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-promise": "^4.2.1",
        "mocha": "^8.2.1",
        "mocha-junit-reporter": "^2.0.0",
        "nodemon": "^2.0.7",
        "nyc": "^15.1.0",
        "prettier": "^2.2.1",
        "shx": "^0.3.3",
        "sinon": "^9.2.3",
        "ts-mockito": "^2.6.1",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3",
        "updtr": "^1.0.0"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "joi": "^17.2.1",
        "logops": "^2.1.1"
    }
}